{% render "c-info" %}

{% schema %}
{
  "name": "Board Tech",
  "class": "page-board-tech",
  "presets": [
    {
      "name": "Board Tech"
    }
  ]
}
{% endschema %}

{% style %}

span.c-info-card__learn-more {
    margin: 0 40px 50px;
    border-radius: 30px;
    padding: 10px 20px;
    font-size: 20px;
    text-transform: none;
    align-items: center;
    display: flex;
    justify-content: space-between;
    line-height: 1;
    @media screen and (max-width: 991px) {
      font-size: 16px;
    }
}

  .page-board-tech {
    padding: 90px 0;
    background: #eec900;
    background-image: url(//capitasnowboarding.com/cdn/shop/files/GRID-BACKGROUND-WHITE_COMPRESS.jpg?v=1745924560);
    background-repeat: repeat;
    background-size: 85%;
  }
  .c-dropdown-num-grid-image__item {
    display: grid;
    gap: 20px;
    align-content: start;
  }
  .head-image-tech {
    border-radius: 20px;
  }
  .c-d-component {
    margin-top: 30px;
  }
  .c-info-card {
    background: #fff;
  }

  @media screen and (min-width: 48em) {
    .page-board-tech {
      padding: 180px 0;
    }
    .c-dropdown-text-tech__list,
    .c-dropdown-num-grid__list,
    .c-dropdown-num-grid-image__list {
      list-style-type: disc;
    }
    .c-dropdown-num-grid-image__item-wrap {
        min-height: 170px;
    }
    .c-dropdown-camber__top {
      flex-direction: column;
    }
    .head-image-tech {
      border-radius: 30px;
    }
  }

  button.btn-transp btn-action.js-board-tech-dropdown-btn {
    padding: 10px 20px;
  }
  button.btn-transp.btn-action.js-board-tech-dropdown-btn {
    width: 100% !important;
    max-width: none;
    border: none;
  }
  .c-dropdown:not(.c-dropdown.is-small) {
      border: 1px solid #000;
      border-radius: 25px;
      background: #fff;
  }
  .c-info .btn-link.btn-black {
    margin: 30px auto;
}

@media screen and (min-width: 48em) {
    .c-info {
        grid-gap: 40px;
        padding-inline: 0;
    }
}

@media screen and (min-width: 48em) {
    .template-board-tech .u-content.wider {
        padding-inline: 0;
    }
}

    .template-board-tech {
      .o-text.is-4 {
        text-transform: capitalize;
        font-weight: 500;
        font-size: 20px;
      }

      button.btn-transp.btn-action:hover,
      button.btn-transp.btn-action.js-board-tech-dropdown-btn.is-active {
        text-decoration: unset !important;
      }
    }

button.btn-transp.btn-action.js-board-tech-dropdown-btn {
  padding: 31.5px 35px;
}

.c-dropdown__content {
  border-top: unset;
}

.c-dropdown.is-small .c-dropdown__content {
    border-top: none;
    padding: 0 20px;
}

button.btn-transp.btn-action.js-board-tech-dropdown-btn:after {
    background: url(data:image/svg+xml;base64,);
    background-repeat: no-repeat;
    background-position: 100%;
    background-size: 27px;
}
button.btn-transp.btn-action.js-board-tech-dropdown-btn.is-active:after {
    background: url(data:image/svg+xml;base64,);
    background-repeat: no-repeat;
    background-position: 100%;
    background-size: 27px;
}

button.btn-transp.btn-action.js-board-tech-dropdown-btn > span.is-4 {
    font-weight: 600;
}

.c-dropdown-num-grid-image__content span.o-text.is-4.is-regular {
  font-size: 16px;
}

.c-dropdown.js-board-tech-dropdown .btn-action {
  min-width: unset;
}

.c-dropdown.is-small.js-board-tech-dropdown > button.btn-transp.btn-action.js-board-tech-dropdown-btn {
  padding: 30px 15px;
}

{% endstyle %}
