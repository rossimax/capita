  </div>
</div>


<div class="u-container mt-5">
  <div class="u-content wider">
  
    <div class="u-toggle">

      <div class="u-toggle__head js-open-toggle">
        <h3 class="strong">Specs</h3>
        <svg width="17" height="17" viewBox="0 0 17 17" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M8.5 16C12.6421 16 16 12.6421 16 8.5C16 4.35786 12.6421 1 8.5 1C4.35786 1 1 4.35786 1 8.5C1 12.6421 4.35786 16 8.5 16Z" stroke="#0B0204" stroke-width="1" stroke-miterlimit="10"/>
          <path d="M5.19971 8.5H11.8003" stroke="#0B0204" stroke-width="1" stroke-miterlimit="10" stroke-linecap="round"/>
          <path class="svg-line-vert" d="M8.49976 5.19971V11.8003" stroke="#0B0204" stroke-width="1" stroke-miterlimit="10" stroke-linecap="round"/>
        </svg>
      </div>
      <div class="u-toggle__content">
          <div class="c-size-chart">


                {% comment %} {{ product.metafields.snowboard_info.dimensions }} {% endcomment %}

                <div class="size-chart-new">
                  <div class="size-chart-new__variants-name">
                    {% for variant in product.variants %}

                      <div class="strong js-variant-size-name c-product-info__size {% if forloop.index == 1 %}active{% endif %}" data-variant-name="{{ variant.title }}">
                        {{ variant.title }}
                      </div>
                      
                    {% endfor %}
                  </div>
                  <div class="size-chart-new__variants-specs">
                    {% for variant in product.variants %}

                      <div class="variant-specs js-variant-size-specs {% if forloop.index != 1 %}hide{% endif %}" data-variant-name="{{ variant.title }}">

                        <div class="variant-specs__row">
                          <span>Effective Edge (mm)</span>
                          <span>{{ variant.metafields.accentuate.effective_edge_mm }}</span>
                        </div>

                        <div class="variant-specs__row">
                          <span>Waist</span>
                          <span>{{ variant.metafields.accentuate.waist }}</span>
                        </div>
                        
                        <div class="variant-specs__row">
                          <span>Nose / Tail</span>
                          <span>{{ variant.metafields.accentuate.nose__tail }}</span>
                        </div>

                        <div class="variant-specs__row">
                          <span>Taper</span>
                          <span>{% if variant.metafields.accentuate.taper == blank %} 0 {% else %} {{ variant.metafields.accentuate.taper }} {% endif %}</span>
                        </div>

                        <div class="variant-specs__row">
                          <span>Sidecut</span>
                          <span>{{ variant.metafields.accentuate.sidecut }}</span>
                        </div>

                        <div class="variant-specs__row">
                          <span>Ref. Stance (cm/in)</span>
                          <span>{{ variant.metafields.accentuate.ref_stance_cmin }}</span>
                        </div>
                        
                        <div class="variant-specs__row">
                          <span>Suggested Weight (lb/kg)</span>
                          <span>{{ variant.metafields.accentuate.suggested_weight_lbkg }}</span>
                        </div>

                        <div class="variant-specs__row">
                          <span>Suggested Boot Size</span>
                          <span>{{ variant.metafields.accentuate.suggested_boot_size }}</span>
                        </div>

                      </div>
                      
                    {% endfor %}
                  </div>
                </div>
          </div>
      </div>
    </div>
  </div>
</div>


