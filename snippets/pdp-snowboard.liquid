          </svg>
      </div>
      <div class="u-toggle__content">
          <div class="c-accolades">
            {% if product.metafields.accentuate.award_title != blank %}
              <span class="c-accolades__title subtitle strong">{{
                product.metafields.accentuate.award_title }}
              </span>
              {% endif %}
              {% if product.metafields.accentuate.award_description.html != blank %}
              <div class="text">
                {{ product.metafields.accentuate.award_description.html }}
              </div>
            {% endif %}
            {% if product.metafields.snowboard_info.award_video != blank %}
              <div class="c-accolades__video">
                <iframe src="https://www.youtube.com/embed/{{ product.metafields.snowboard_info.award_video }}"
                  title="Capita Award | {{ product.metafields.accentuate.award_title }}" frameborder="0"
                  allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                  allowfullscreen="allowfullscreen"></iframe>
              </div>
            {% endif %}
          </div>
      </div>
