<div class="u-container mt-5">
  <div class="u-content wider">
  
    <div class="u-toggle">

      <div class="u-toggle__head js-open-toggle">
        <h3 class="strong">Construction</h3>
        <svg width="17" height="17" viewBox="0 0 17 17" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M8.5 16C12.6421 16 16 12.6421 16 8.5C16 4.35786 12.6421 1 8.5 1C4.35786 1 1 4.35786 1 8.5C1 12.6421 4.35786 16 8.5 16Z" stroke="#0B0204" stroke-width="1" stroke-miterlimit="10"/>
          <path d="M5.19971 8.5H11.8003" stroke="#0B0204" stroke-width="1" stroke-miterlimit="10" stroke-linecap="round"/>
          <path class="svg-line-vert" d="M8.49976 5.19971V11.8003" stroke="#0B0204" stroke-width="1" stroke-miterlimit="10" stroke-linecap="round"/>
          </svg>
      </div>
      <div class="u-toggle__content">

        <div class="construction-picture c-constructions">

          {% for hotspot in product.metafields.construction_hotspot.board_construction_hotspot_title %}
            <div class="c-hotspot" style="
                --x-mob: {{ product.metafields.construction_hotspot.coordinates_x_mob[forloop.index0] }}; 
                --x-desk: {{ product.metafields.construction_hotspot.coordinates_x_desk[forloop.index0] }}; 
                --y-mob: {{ product.metafields.construction_hotspot.coordinates_y_mob[forloop.index0] }}; 
                --y-desk: {{ product.metafields.construction_hotspot.coordinates_y_desk[forloop.index0] }};
                " data-title="{{ hotspot }}"
              data-descr="{{ product.metafields.construction_hotspot.board_construction_hotspot_description[forloop.index0].html }}"
              data-image="{{ product.metafields.construction_hotspot.board_construction_hotspot_image[forloop.index0] }}"
              data-image-icon="{{ product.metafields.construction_hotspot['3d_board_construction_hotspot_image_icon'][forloop.index0] }}">
              <div class="c-hotspot__btn js-constructions-hotspot-btn">
                <div class="construct-data-num">{{ forloop.index }}</div>
              </div>
            </div>
            {% endfor %}

            <picture><img class="c-constructions__image" src="{{ product.metafields.construction_hotspot['3d_board_image'] }}" /></picture>

        </div>

        <div class="construction-datas ">
          {% for hotspot in product.metafields.construction_hotspot.board_construction_hotspot_title %}

            <div class="construction-data-single">
              <div class="construction-data-single__content">
                <div class="construct-data-num">{{ forloop.index }}</div>
                <img src="{{ product.metafields.construction_hotspot['3d_board_construction_hotspot_image_icon'][forloop.index0] }}" alt="">
                <div class="construct-data-title strong"> {{ hotspot }} </div>
              </div>
              <div class="construction-data-single__desc text">{{ product.metafields.construction_hotspot.board_construction_hotspot_description[forloop.index0].html }}</div>
              {% if product.metafields.construction_hotspot.board_construction_hotspot_image[forloop.index0] %}
                <img class="hotspot-image-constr" src="{{ product.metafields.construction_hotspot.board_construction_hotspot_image[forloop.index0] }}" alt="">
              {% endif %}
            </div>
          {% endfor %}
        </div>
        
      </div>
    </div>
  </div>
</div>


<div class="u-container mt-5">
  <div class="u-content wider">
  
    <div class="u-toggle">

      <div class="u-toggle__head js-open-toggle">
        <h3 class="strong">Intel</h3>
        <svg width="17" height="17" viewBox="0 0 17 17" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M8.5 16C12.6421 16 16 12.6421 16 8.5C16 4.35786 12.6421 1 8.5 1C4.35786 1 1 4.35786 1 8.5C1 12.6421 4.35786 16 8.5 16Z" stroke="#0B0204" stroke-width="1" stroke-miterlimit="10"/>
